link:- https://www.geeksforgeeks.org/reverse-a-stack-using-recursion/

#include <iostream>
#include <stdlib.h>
using namespace std;
#define n 5
int stack[n],res[n];
int top=-1;
void push(int x)
{
    if(top==n)
    cout<<"stack overflow";
    else
    {
        
        top++;
        stack[top]=x;
    }
}
void pop()
{
    if(top==-1)
    cout<<"stack is empty";
    else
    {
        res[top]=stack[top];
        stack[top]='\0';
        top--;
    }
    
}
void fun()
{
    if(top!=-1)
    {
       pop();
       fun();
    }
}
int main() 
{
  push(10);
  push(20);
  push(30);
  fun();
  for(int i=top+1;i<(n-1);i++)
  
  {
      if(res[i]!=0)
      cout<<res[i]<<" ";
  }
	return 0;
}
